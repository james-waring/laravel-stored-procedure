{
    "name": "jlw/laravel-stored-procedure",
    "description": "Add a proc() method to Eloquent Query Builder for calling stored procedures.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JLW\\StoredProcedure\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JLW\\StoredProcedure\\StoredProcedureServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "orchestra/testbench": "^9.14"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
